          },
          {
            "@type": "GovernmentService",
            "@id": "https://www.maharentagreement.com/#service",
            "name": "Registered Rent Agreement E-Registration",
            "provider": {
              "@id": "https://www.maharentagreement.com/#organization"
            },
            "serviceType": "Rent Agreement Registration",
