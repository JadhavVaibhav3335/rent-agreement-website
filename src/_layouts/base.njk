    <meta name="robots" content="index, follow"/>
    <script type="application/ld+json">
      {
        "@context": "https://schema.org",
        "@graph": [
          {
            "@type": "Organization",
            "@id": "https://www.maharentagreement.com/#organization",
            "name": "MahaRentAgreement",
            "url": "https://www.maharentagreement.com/",
